try:
    # connect to the server
    print 'Going for adding server to cluster'
    connect(r'[% wl_username %]', r'[% wl_password %]', r'[% admin_url %]')
    
    serverName = r'[% server_name %]'
    clusterName = r'[% cluster_name %]'
    edit()
    startEdit(-1,-1,'false')
    cd('/')
    server = cmo.createServer(serverName)
    server.setCluster(getMBean('/Clusters/' + clusterName))
    save()
    activate()
except WLSTException:
    print 'Adding server to cluster exception'
